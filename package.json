{
  "name": "vdom",
  "version": "1.0.0",
  "main": "index.js",
  "module": "index.es6.js",
  "author": "VK",
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "test": "nyc mocha *_test.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "pretty": "find . -name '*.js' | grep -v node_modules | xargs yarn prettier --write",
    "stream_server": "node example/stream/static_server.jsm 8081",
    "stream": "yarn webpack -o example/stream/app.dist.js --mode development example/stream/app.js"
  },
  "nyc": {
    "es-modules": true,
    "cache": false,
    "clean": true,
    "all": true,
    "instrument": true,
    "include": [
      "**"
    ],
    "exclude": [
      "expect.js",
      "vdom_test.js",
      "example/**"
    ],
    "sourceMap": false
  },
  "devDependencies": {
    "codecov": "^3.6.5",
    "esm": "^3.2.25",
    "git": "^0.1.5",
    "jsdom": "^15.2.1",
    "jsdom-simulant": "^1.1.2",
    "mime": "^1.4.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "serialize-javascript": "^2.1.1",
    "sinon": "^8.0.4",
    "streams": "https://github.com/dotchain/streams.git",
    "uuid": "^3.3.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "yarn": "^1.21.1"
  }
}
